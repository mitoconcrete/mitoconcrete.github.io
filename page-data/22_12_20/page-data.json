arn"}}},{"node":{"id":"cfaae3b8-2461-5499-9abc-275be24fc339","fields":{"slug":"/23_01_16/"},"frontmatter":{"title":"✏️ 2023-01-16 Today I Learn"}}},{"node":{"id":"3e6dc3a0-187c-5a85-96da-646a42b0c46c","fields":{"slug":"/23_01_17/"},"frontmatter":{"title":"✏️ 2023-01-17 Today I Learn"}}},{"node":{"id":"cee48820-6a40-5bf2-931d-a0969b10f30a","fields":{"slug":"/23_01_18/"},"frontmatter":{"title":"✏️ 2023-01-18 Today I Learn"}}},{"node":{"id":"3c4f4476-88b8-5faa-80de-77fa9613e727","fields":{"slug":"/23_01_19/"},"frontmatter":{"title":"✏️ 2023-01-19 Today I Learn"}}}]},"previous":{"fields":{"slug":"/22_12_19/"},"frontmatter":{"title":"✏️ 2022-12-19 Today I Learn"}},"next":{"fields":{"slug":"/22_12_21/"},"frontmatter":{"title":"✏️ 2022-12-21 Today I Learn"}}},"pageContext":{"id":"11b54ad8-d2df-5a32-b3e2-6b898a7de696","series":"내일배움캠프","previousPostId":"b3af73e4-a037-561f-b94a-508de5950c09","nextPostId":"d0703451-e03f-511b-89da-50cec81c2988"}},"staticQueryHashes":[]}